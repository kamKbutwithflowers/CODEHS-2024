function start(){
    runtoPancakes();
    putPancakes();
    main();
    putPancakes();
    move();
    turnRight();
}

/*
* Precondition:starting point,Karel is facing east
* Postcondition: Karel is inline to drop the balls
*/
function runtoPancakes(){
    move();
    turnLeft();
}

function putPancakes(){
    putBall();
    move();
    putBall();
    move();
    putBall();
}

function main(){
    turnRight();
    move();
    turnRight();
    move();
    move();
    turnLeft();
    move();
    turnLeft();
}

function turnRight(){
    turnLeft();
    turnLeft();
    turnLeft();
}
