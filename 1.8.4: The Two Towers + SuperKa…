function start(){
    runtoPancakes();
    putPancakes();
    main();
    putPancakes();
    move();
    turnRight();
}

/*
* allows karel to get in line to drop the balls
*/
function runtoPancakes(){
    move();
    turnLeft();
}

/*
* Karel drops all 3 balls in the appropriate position
*/
function putPancakes(){
    putBall();
    move();
    putBall();
    move();
    putBall();
}

function main(){
    turnRight();
    move();
    turnRight();
    move();
    move();
    turnLeft();
    move();
    turnLeft();
}
